<div id="zip_code_range_members" data-hook="member" class="omega six columns">
  <fieldset class="no-border-bottom">
    <legend align="center"><%= Spree.t(:zip_code_ranges) %></legend>

    <%= zone_form.field_container :zip_code_range_ids do %>
      <%= zone_form.label :zip_code_range_ids, Spree.t(:zip_code_ranges) %><br>
      <%= zone_form.collection_select :zip_code_range_ids, @zip_code_ranges, :id, :name, {}, { :multiple => true, :class => "select2 fullwidth" } %>
    <% end %>
  </fieldset>
</div>
